CÓDIGO FONTE
while True:
  try:
    j = raw_input().split()
    a = j[0]
    b = j[1]
    c = j[2]
    a = int(a)
    b = int(b)
    c = int(c)
    if a == b == c:
      print "*"
    if a != b and a != c:
      print "A"
    if b != a and b != c:
      print "B"
    if c != a and c != b:
      print "C"
  except:
    break
